{%- style -%}
  header-drawer {
    justify-self: start;
  }
  {%- if section.settings.head_shadow == 'head_shadow_all' -%}
  .header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }
  {%- endif -%}
  {%- if section.settings.head_shadow == 'head_shadow_home' -%}
  .header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }
  .page-index .header-wrapper {
    box-shadow: none;
  }
  {%- endif -%}
  .shopify-section-header-sticky sticky-header.header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }
  {%- if section.settings.sticky_header_type == 'reduce-logo-size' and section.settings.head_shadow == 'head_shadow_home' -%}
    .page-index .shopify-section-header-sticky sticky-header.header-wrapper {
      box-shadow: none;
    }
  {%- endif -%}
  {%- if section.settings.sticky_header_type == 'always' and section.settings.head_shadow == 'head_shadow_home' -%}
    .page-index .shopify-section-header-sticky sticky-header.header-wrapper {
      box-shadow: none;
    }
  {%- endif -%}
  .shopify-section-header-sticky.scrolled-past-header sticky-header.header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }

  {%- if section.settings.logo_position == 'top-left' -%}
    .psearch-hidden {
      overflow: hidden;
    }
    .psearch-hidden:before{
      content: '';
      position: fixed;
      z-index: 3;
      left: 0;
      top: 0;
      width: 100vw;
      height: 100%;
      display: flex;
      justify-content: flex-end;
      background-color: rgba(var(--color-foreground),.3);
    }
    .psearch-hidden .section-header{
      position: unset;
    }
    .psearch-hidden .header--top-left .header__search {
      z-index: 3;
    }
    .psearch-hidden .section-header.shopify-section-header-sticky{
      position: sticky;
      top: 0;
    }
  {%- endif -%}
  {%- if section.settings.sticky_header_type == 'reduce-logo-size' -%}
    .scrolled-past-header .header__heading-logo-wrapper {
      width: 75%;
    }
  {%- endif -%}
    .header{
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  @media screen and (min-width: 992px) {
    header-drawer.mobile-drawer {
      display: none;
    }
  }

  {%- if section.settings.menu == blank -%}
    @media screen and (min-width: 992px) {
      .gradient.wbmenuup.header__inline-menu {
        padding: 0;
      }
    }
  {%- endif -%}

  @media screen and (max-width: 991px) {
    .header.header--top-left details-modal {
      margin-top: {{ section.settings.padding_top | times: 0.5 | round: 0 }}px;
    }
    .header--top-left .predictive-search.predictive-search--header,
    .header--middle-left.search-full-width .predictive-search.predictive-search--header {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  .section-header {
    position: sticky;
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }
  predictive-search:not([loading]) .predictive-search__loading-state, predictive-search:not([loading]) .predictive-search-status__loading {
      display: none;
  }
  @media screen and (min-width: 768px) {
    .section-header {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }

{%- endstyle -%}

