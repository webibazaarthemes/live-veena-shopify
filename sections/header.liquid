{%- liquid
  for block in section.blocks
    if block.type == '@app'
      assign has_app_block = true
    endif
    if block.type == 'categories'
      assign menu__position = block.settings.menu_position
    endif
    if block.type == 'second-menu'
      assign secondary__menu = block.settings.secmenu
      assign showsmenu_top = block.settings.showsmenu_top
    endif

    if block.type == 'categories' and block.settings.main_mega_menu != blank
      assign tabmenu_hide = 'not-hidden'
      assign flex-menu = 'flex-menu'
    endif

  endfor
-%}

<{% if section.settings.sticky_header_type != 'none' %}sticky-header data-sticky-type="{{ section.settings.sticky_header_type }}"{% else %}div{% endif %} class="header-wrapper color-{{ section.settings.color_scheme }} gradient header-wrapper--border-bottom dropm-menu">
<div class="page-width">
  <header class="header header--{{ section.settings.logo_position }} header--menu-{{ menu__position }}{% if section.settings.logo_position == 'middle-left' and section.settings.menu == blank %} search-full-width{% endif %} header--mobile-left{% if section.settings.menu != blank %} header--has-menu{% endif %}{% if has_app_block %} header--has-app{% endif %}{% if settings.wb_cartnumber == 'wb_shoptotal' or settings.wb_cartnumber == 'wb_shopqtytotal' %} wbcartbth{% endif %}">
      <header-drawer data-breakpoint="tablet" class="mobile-drawer">
        <details id="Details-menu-drawer-container" class="menu-drawer-container">
          <summary class="header__icon header__icon--menu header__icon--summary link focus-inset" aria-label="{{ 'sections.header.menu' | t }}">
            <span>
              {% render 'icon-hamburger' %}
              {% render 'icon-close' %}
            </span>
          </summary>
            <div id="menu-drawer" class="{{ flex-menu }} color-{{ section.settings.color__dropmenu }} gradient menu-drawer motion-reduce" tabindex="-1">
              <ul class="mobilemenu-tab {{ tabmenu_hide }} {% if section.settings.menu == blank %} hidden {% endif %}">
                <li class="nav-item active" data-id="menu1">{{ 'sections.header.menu' | t }}</li>
                <li class="nav-item" data-id="menu2">{{ 'wbcustomlabel.wballtext.category' | t }}</li>
              </ul>
              <div>
                <div id="menu1" class="mobile_menu_tab menu-drawer__inner-container tab-pane fade {% if section.settings.menu != blank %} active {% endif %}">
                  <div class="menu-drawer__navigation-container">
                    <nav class="menu-drawer__navigation">
                      <ul class="menu-drawer__menu has-submenu list-menu">
                        {%- for link in section.settings.menu.links -%}
                          {%- liquid 
                            assign assignsubmenu = true
                            assign blank_link = false
                            assign onlymenulink = false
                            for block in section.blocks
                               assign case_title = block.settings.menuname | downcase
                               assign lower_title = link.title | downcase
                               if lower_title == case_title
                                   if block.settings.showsubmenu == false
                                       assign assignsubmenu = false
                                       assign onlymenulink = true
                                   endif
                                   if link.links == blank 
                                       assign blank_link = lower_title
                                   endif
                               endif
                           endfor 
                          -%}
                          <li>
                            {%- if link.links != blank -%}
                              {% if onlymenulink %}
                                <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                  {{ link.title | escape }}
                                </a>
                              {% endif %}
                              {% if assignsubmenu %}
                                <details id="Details-menu-drawer-menu-item-{{ forloop.index }}">
                                  <summary class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.child_active %} menu-drawer__menu-item--active{% endif %}">
                                    {{ link.title | escape }}
                                    {% render 'icon-arrow' %}
                                    {% render 'icon-caret' %}
                                  </summary>
                                  <div id="link-{{ link.handle | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce" tabindex="-1">
                                    <div class="menu-drawer__inner-submenu">
                                      <button class="menu-drawer__close-button link link--text focus-inset" aria-expanded="true">
                                        {% render 'icon-arrow' %}
                                        {{ link.title | escape }}
                                      </button>
                                      <ul class="menu-drawer__menu list-menu" tabindex="-1">
                                        {% if assignsubmenu %}
                                          {%- for childlink in link.links -%}
                                            <li>
                                              {%- if childlink.links == blank -%}
                                                <a href="{{ childlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if childlink.current %} menu-drawer__menu-item--active{% endif %}"{% if childlink.current %} aria-current="page"{% endif %}>
                                                  {{ childlink.title | escape }}
                                                </a>
                                              {%- else -%}
                                                <details>
                                                  <summary class="menu-drawer__menu-item link link--text list-menu__item focus-inset">
                                                    {{ childlink.title | escape }}
                                                    {% render 'icon-arrow' %}
                                                    {% render 'icon-caret' %}
                                                  </summary>
                                                  <div id="childlink-{{ childlink.handle | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce">
                                                    <button class="menu-drawer__close-button link link--text focus-inset" aria-expanded="true">
                                                      {% render 'icon-arrow' %}
                                                      {{ childlink.title | escape }}
                                                    </button>
                                                    <ul class="menu-drawer__menu list-menu" tabindex="-1">
                                                      {%- for grandchildlink in childlink.links -%}
                                                        <li>
                                                          <a href="{{ grandchildlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if grandchildlink.current %} menu-drawer__menu-item--active{% endif %}"{% if grandchildlink.current %} aria-current="page"{% endif %}>
                                                            {{ grandchildlink.title | escape }}
                                                          </a>
                                                        </li>
                                                      {%- endfor -%}
                                                    </ul>
                                                  </div>
                                                </details>
                                              {%- endif -%}
                                            </li>
                                          {%- endfor -%}
                                        {% endif %}
                                        {% render 'megamenu', blocks: section.blocks, title: link.title %}
                                      </ul>
                                    </div>
                                  </div>
                                </details>
                              {% endif %}
                            {%- else -%}
                              {% if blank_link %}
                                {% if onlymenulink %}
                                  <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                  {{ link.title | escape }}
                                  </a>
                                {% endif %}
                                {% if assignsubmenu %}
                                  <details id="Details-menu-drawer-menu-item-{{ forloop.index }}">
                                      <summary class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.child_active %} menu-drawer__menu-item--active{% endif %}">
                                        {{ link.title | escape }}
                                        {% render 'icon-arrow' %}
                                        {% render 'icon-caret' %}
                                      </summary>
                                      <div id="link-{{ link.title | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce" tabindex="-1">
                                        <div class="menu-drawer__inner-submenu">
                                            <button class="menu-drawer__close-button link link--text focus-inset" aria-expanded="true">
                                              {% render 'icon-arrow' %}
                                              {{ link.title | escape }}
                                            </button>
                                            <div class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                              {% render 'megamenu', blocks: section.blocks, title: link.title %}
                                            </div>
                                        </div>
                                      </div>
                                  </details>
                                {% endif %}
                                  {% assign blank_link = false %}
                                {% else %}
                                  <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                  {{ link.title | escape }}
                                  </a>
                              {% endif %}
                            {%- endif -%}
                          </li>
                        {%- endfor -%}
                      </ul>
                    </nav>
                  </div>
                </div>
